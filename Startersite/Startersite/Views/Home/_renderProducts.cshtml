@model IEnumerable<Startersite.Products>

@foreach (var el in Model)
{
    <div class="container">
        <table class="table table-hover">
            <tr>
                <td>@el.ProductName</td>
                <td>@el.Description</td>
                <td>@el.Price</td>
                <td><button>Add to cart</button></td>
            </tr>
        </table>
    </div>
}
